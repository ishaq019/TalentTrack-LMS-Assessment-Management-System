              "outputFormat": "i j",
              "constraints": "2 <= n <= 2*10^5",
              "languages": ["javascript", "python"],
              "starterCode": {
                "javascript": "const fs = require('fs');\nconst data = fs.readFileSync(0,'utf8').trim().split(/\\s+/);\nlet k=0;\nconst n = Number(data[k++]);\nconst arr = [];\nfor (let i=0;i<n;i++) arr.push(Number(data[k++]));\nconst target = Number(data[k++]);\n// TODO: print i and j\n",
                "python": "import sys\ndata = sys.stdin.read().strip().split()\nk=0\nn=int(data[k]); k+=1\narr=list(map(int,data[k:k+n])); k+=n\ntarget=int(data[k])\n# TODO: print i and j\n"
              },
              "testcases": [
                { "input": "4\n2 7 11 15\n9\n", "output": "0 1\n", "isSample": true },
